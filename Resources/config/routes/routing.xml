<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="morph_data_import_entities_list" path="/" methods="GET"
           controller="WideMorph\Morph\Bundle\MorphDataImportBundle\Presentation\Controller\IndexController::entitiesList">
    </route>
    <route id="morph_data_import_create_import_plan" path="/{entityName}/create-import-plan" methods="GET"
           controller="WideMorph\Morph\Bundle\MorphDataImportBundle\Presentation\Controller\IndexController::createCreateImportPlan">
    </route>
    <route id="morph_data_import_entity_import" path="/{entityName}/import" methods="GET"
           controller="WideMorph\Morph\Bundle\MorphDataImportBundle\Presentation\Controller\IndexController::entityImport">
    </route>

    <route id="api_morph_data_import" path="/api/import/data" methods="POST"
           controller="WideMorph\Morph\Bundle\MorphDataImportBundle\Presentation\Controller\Api\ImportController::import">
    </route>

</routes>
